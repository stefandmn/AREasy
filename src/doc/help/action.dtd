<!DOCTYPE help [
		<!ELEMENT help (name|description|syntax|options|options1|options2|samples)*>
		<!ATTLIST help
				inherits CDATA #IMPLIED>
		<!ELEMENT name (#PCDATA)>
		<!ELEMENT syntax (#PCDATA)>
		<!ELEMENT description (#PCDATA)>
		<!ELEMENT options (option)*>
		<!ELEMENT options1 (option)*>
		<!ELEMENT options2 (option)*>
		<!ELEMENT samples (sample)*>
		<!ELEMENT option (#PCDATA)>
		<!ELEMENT sample (#PCDATA)>
		<!ATTLIST option
				key CDATA #REQUIRED
				type (string|number|date|bool|list) #REQUIRED
				defvalue CDATA #IMPLIED
				selvalues CDATA #IMPLIED
				description CDATA #IMPLIED>
		<!ATTLIST sample
				code CDATA #REQUIRED
				description CDATA #REQUIRED>
]>
